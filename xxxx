local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://10723356676",
    PremiumOnly = false
})

MainTab:AddToggle({
    Name = "FreezeTrade",
    Default = false,
    Callback = function(v)
        print("Dupe: " .. tostring(v))
    end
})

MainTab:AddSlider({
    Name = "Module",
    Min = 2,
    Max = 4,
    Default = 1,
    Callback = function(v)
        print("Slider 1 set to: " .. tostring(v))
    end
})

MainTab:AddButton({
