local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
    IntroText = "GUI by zaliveeee",
    Name = "ZaliGUI | by zaliveeee",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest",
    Icon = "https://avatars.mds.yandex.net/i?id=ffa1e92e8483a36b8a29353d4b35a616a20bc282-13308716-images-thumbs&n=13"
})

local AutoFarmTab = Window:MakeTab({
    Name = "Auto Farm",
    Icon = "rbxassetid://10723356676",
    PremiumOnly = false
})

local AutoFarmSection = AutoFarmTab:AddSection({
    Name = "Auto Farm üíµ"
})

AutoFarmTab:AddButton({
    Name = "On",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Auto Farm On",
            Content = "Onüíö",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

AutoFarmTab:AddButton({
    Name = "Off",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Auto Farm Off",
            Content = "Off‚ùó",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

local TradeScamTab = Window:MakeTab({
    Name = "Trade-Scamü§©",
    Icon = "rbxassetid://10723356676",
    PremiumOnly = false
})

local TradeSection = TradeScamTab:AddSection({
    Name = "Trade Scam"
})

local playerName = ""

TradeScamTab:AddTextbox({
    Name = "Freeze Playerüòá",
    Default = "",
    TextDisappear = true,
    Callback = function(value)
        playerName = value
        OrionLib:MakeNotification({
            Name = "Name Set",
            Content = "You entered: " .. playerName,
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

TradeScamTab:AddButton({
    Name = "On",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Freeze Player",
            Content = "Onüíö",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

TradeScamTab:AddButton({
    Name = "Off",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Freeze Player",
            Content = "Off‚ùó",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

TradeScamTab:AddToggle({
    Name = "Auto Accept Trade‚úÖ",
    Default = false,
    Callback = function(state)
        if state then
            OrionLib:MakeNotification({
                Name = "Trade Accepted",
                Content = "Auto Accept: Onüíö",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Trade Disabled",
                Content = "Auto Accept: Off‚ùó",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://10723356676",
    PremiumOnly = false
})

MainTab:AddToggle({
    Name = "FreezeTrade",
    Default = false,
    Callback = function(v)
        print("Dupe: " .. tostring(v))
    end
})

MainTab:AddSlider({
    Name = "Module",
    Min = 2,
    Max = 4,
    Default = 2,
    Callback = function(v)
        print("Slider 1 set to: " .. tostring(v))
    end
})

MainTab:AddButton({
    Name = "By zaliveeee https://discord.gg/25HpqwgDak",
    Callback = function()
        setclipboard("https://discord.gg/25HpqwgDak")
        OrionLib:MakeNotification({
            Name = "Copied!",
            Content = "Discord link copied to clipboard.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})
