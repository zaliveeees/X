local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
    IntroText = "GUI by zaliveeee",
    Name = "ZaliGUI | by zaliveeee",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest",
    Icon = "https://avatars.mds.yandex.net/i?id=ffa1e92e8483a36b8a29353d4b35a616a20bc282-13308716-images-thumbs&n=13"
})

local AutoFarmTab = Window:MakeTab({
    Name = "Auto Farm",
    Icon = "rbxassetid://10723356676",
    PremiumOnly = false
})

local AutoFarmSection = AutoFarmTab:AddSection({
    Name = "Auto Farm üíµ"
})

AutoFarmTab:AddButton({
    Name = "Start Auto Farm",
    Callback = function()
        spawn(function()
            repeat wait() until game:IsLoaded()
            
            local player = game.Players.LocalPlayer
            local humanoid = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            
            function teleportTo(pos)
                if humanoid then
                    humanoid.CFrame = CFrame.new(pos)
                end
            end
            
            function collectMoney()
                for _, v in pairs(game:GetService("Workspace").Ignored:GetChildren()) do
                    if v:IsA("Part") and v:FindFirstChild("TouchInterest") then
                        teleportTo(v.Position)
                        wait(0.5)
                    end
                end
            end
            
            function autoCare()
                local petNeeds = {"Sleep", "Shower", "Hunger", "Thirst"}
                for _, need in pairs(petNeeds) do
                    local target = game:GetService("Workspace"):FindFirstChild(need)
                    if target then
                        teleportTo(target.Position)
                        wait(2)
                    end
                end
            end
            
            while true do
                collectMoney()
                autoCare()
                wait(10)
            end
        end)
    end
})

AutoFarmTab:AddToggle({
    Name = "Enable Auto Farm",
    Default = false,
    Callback = function(state)
        if state then
            OrionLib:MakeNotification({
                Name = "Auto Farm",
                Content = "Enabled",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Auto Farm",
                Content = "Disabled",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})

